<!--miniprogram/pages/index/canvas.wxml-->
<text>miniprogram/pages/index/canvas.wxml</text>
<view>
  <button bindtap="bindAddLinkTap">add links</button>
  <button bindtap="bindViewDone">done</button>
  <image class="pic"></image>
  <block wx:for="{{links}}">
    <view ></view>
    <link style="position:absolute; left:{{item.x}}px; top:{{item.y}}px" paramA="{{index}}"></link>
  </block>
</view>